{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DetailedErrors": true,
  "StorageOptions": {
    "Provider": "SQLite",
    "DatabasePath": "settings/azurenamingtool.db",
    "EnableAutoMigration": true,
    "BackupRetentionDays": 30
  }
}
